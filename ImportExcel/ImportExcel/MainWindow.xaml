<Window x:Class="ImportExcel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ImportExcel"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid IsEnabled="true">
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="25" Margin="32,66,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="298" ToolTip="file path"/>
        <Button x:Name="button" Content="Get File" HorizontalAlignment="Left" Margin="350,66,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <DataGrid x:Name="dataGrid" HorizontalAlignment="Left" Margin="32,115,0,0" VerticalAlignment="Top" Height="178" Width="298"/>
        <Button x:Name="button1" Content="Upload" HorizontalAlignment="Left" Margin="350,115,0,0" VerticalAlignment="Top" Width="75" IsEnabled="False" Click="button1_Click"/>

    </Grid>
</Window>
